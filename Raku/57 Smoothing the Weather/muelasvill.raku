# raku -c muelasvill.raku

sub main () {
    
    my $object = prompt;
    my $cantOfNumbers = $object.Int;
    my @numbers = prompt().split(" ");
    print @numbers[0];
    print " ";
    SmoothingtheWeather(1, $cantOfNumbers - 1, @numbers);

}


sub SmoothingtheWeather($i, $cantOfnumbers, @numbers) {
    
    if $i == $cantOfnumbers {
        print @numbers[$i];
        return;
    }

    my $average = (@numbers[$i] + @numbers[$i - 1] + @numbers[$i + 1]) / 3;
    print $average;
    print " ";
    
    SmoothingtheWeather($i + 1, $cantOfnumbers, @numbers);

}

main()

